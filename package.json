{
  "name": "linter-recess",
  "main": "./lib/init",
  "linter-package": true,
  "version": "0.0.1",
  "description": "Lint LESS on the fly, using recess (not ready)",
  "repository": "https://github.com/AtomLinter/linter-recess",
  "license": "MIT",
  "engines": {
    "atom": ">0.50.0"
  },
  "dependencies": {
    "recess": "1.1.9"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  }
}
